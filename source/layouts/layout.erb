          <li><a href="/html/video/index.html"  hidefocus="true">Video</a></li>
          <li><a href="/html/geolocation/index.html"  hidefocus="true">Geolocation</a></li>
          <li><a href="/html/websockets/index.html"  hidefocus="true">WebSockets</a></li>
          <li><a href="/html/forms/index.html">Forms</a></li>
          <li><a href="/html/forms/register.html">register</a></li>
        </ul>
      </div>
      <div id="content-right" class="content-right">
        <%= yield %>
      </div>
    </div>
  </body>
</html>